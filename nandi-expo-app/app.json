{
  "expo": {
    "name": "Nandi Test App",
    "slug": "nandi-test-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hbellala.nandi"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.hbellala.nandi",
      "versionCode": 2,
      "permissions": [
        "com.android.vending.BILLING"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "04d9f2a7-b499-4ec2-bd3d-7dfa9db8f722"
      }
    },
    "plugins": [
      [
        "react-native-iap",
        {
          "enablePromiseAPI": true
        }
      ]
    ]
  }
}
